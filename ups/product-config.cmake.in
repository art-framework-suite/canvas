

@PACKAGE_INIT@

if (IS_DIRECTORY "${PACKAGE_PREFIX_DIR}/@product@/@version@/Modules")
  list(APPEND CMAKE_MODULE_PATH "${PACKAGE_PREFIX_DIR}/@product@/@version@/Modules")
endif()

set_and_check(@PROJECT_NAME@_LIBDIR "${PACKAGE_PREFIX_DIR}/lib")
set_and_check(@PROJECT_NAME@_INCDIR "${PACKAGE_PREFIX_DIR}/include")
find_library(CANVAS NAMES canvas PATHS "${@PROJECT_NAME@_LIBDIR}" NO_DEFAULT_PATH)
include_directories( ${@PROJECT_NAME@_INCDIR} )

check_required_components(@product@)

